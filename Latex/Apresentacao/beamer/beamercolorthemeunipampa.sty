\mode<presentation>


\definecolor{verdeUnipampa}{HTML}{0F3751}
\definecolor{verdeUnipampaClaro}{HTML}{EBEEE8}

\definecolor{alertaHead}{HTML}{E1E1D7}
\definecolor{alertaBody}{HTML}{EEF0EA}
\definecolor{alertaTitle}{HTML}{6B6739}


\definecolor{exemploHead}{HTML}{D3D3DD}
\definecolor{exemploBody}{HTML}{EAE9EF}



% Settings
\setbeamercolor*{title page header}{fg=verdeUnipampa}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{item}{fg=verdeUnipampa}
\setbeamercolor*{block}{fg=verdeUnipampa}
\setbeamercolor*{title}{fg=verdeUnipampa}


\setbeamercolor*{numeroSlide}{fg=verdeUnipampa}


%%blocos
\setbeamercolor{block title}{use=structure,fg=white,bg=verdeUnipampa}
\setbeamercolor{block body}{use=structure,fg=black,bg=verdeUnipampaClaro}

\setbeamercolor{block title example}{use=structure,fg=black,bg=exemploHead}
\setbeamercolor{block body example}{use=structure,fg=black,bg=exemploBody}

\setbeamercolor{block title alerted}{use=structure,fg=alertaTitle,bg=alertaHead}
\setbeamercolor{block body alerted}{use=structure,fg=black,bg=alertaBody}



\setbeamercolor{section in toc}{fg=black}








\mode
<all>